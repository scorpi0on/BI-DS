      <Query>
        <DataSourceName>ConnectNBACube</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>WITH
MEMBER [Measures].[Top offense_index]AS
{
    EXISTING
    TopCount([Teams State].[Team].[Team].ALLMEMBERS,1,
        ([Measures].[offense_index])
    ) *
    {[Measures].[offense_index]}
}.Item(0)


